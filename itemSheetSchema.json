{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "title": "sofa",
            "description": "Almost brand new",
            "location": "Oulu",
            "price": "20",
            "deliveryType": "pickup",
            "sellerInfo": "foo@bar.com"
        }
    ],
    "required": [
        "title",
        "description",
        "location",
        "price",
        "deliveryType",
        "sellerInfo"
    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "sofa"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Almost brand new"
            ]
        },
        "location": {
            "$id": "#/properties/location",
            "type": "string",
            "title": "The location schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Oulu"
            ]
        },
        "price": {
            "$id": "#/properties/price",
            "type": "string",
            "title": "The price schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "20"
            ]
        },
        "deliveryType": {
            "$id": "#/properties/deliveryType",
            "type": "string",
            "title": "The deliveryType schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pickup"
            ]
        },
        "sellerInfo": {
            "$id": "#/properties/sellerInfo",
            "type": "string",
            "title": "The sellerInfo schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "foo@bar.com"
            ]
        }
    },
    "additionalProperties": true
}